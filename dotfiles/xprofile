
export SSH_AUTH_SOCK=$XDG_RUNTIME_DIR/ssh_auth_sock

if [[ -n "$(which systemctl)" ]]
then
    systemctl --user import-environment DISPLAY
    systemctl --user import-environment SSH_AUTH_SOCK
fi

if [ "${PATH%%:*}" != "$HOME/bin" ]
then
    if [ -d "$HOME/bin" ]
    then
        PATH="$HOME/bin:$PATH"
    fi
fi

compton&
